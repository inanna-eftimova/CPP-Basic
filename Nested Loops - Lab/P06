#include <iostream>

using namespace std;

int main()
{
    int e; cin>>e;
    int n; cin>>n;
    for (int i = e; i >= 1; i--) {
        for (int j = 0; j < n; j++) {
            if(i==e || e==1){
                cout<<"L"<<i<<j<<" ";
            }else if(i%2==0){
                cout<<"O"<<i<<j<<" ";
            }else{
                cout<<"A"<<i<<j<<" ";
            }
            
        }
        cout<<""<<endl;
        
    }

    return 0;
}
