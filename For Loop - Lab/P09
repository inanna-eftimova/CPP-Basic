#include <iostream>
#include <bits/stdc++.h>
#include <cmath>  
using namespace std;

int main()
{
   int leftSum = 0;
   int rightSum = 0;
   int n; cin>>n;
   for (int i = 0; i < n; i++) {
       int num; cin>>num;
       leftSum+=num;
   }
   for (int i = 0; i < n; i++) {
       int num; cin>>num;
       rightSum+=num;
   }
   if(leftSum==rightSum){
       cout<<"Yes, sum = "<<rightSum<<endl;
   }else{
       cout<<"No, diff = "<<abs(rightSum-leftSum)<<endl;
   }
    return 0;
}
