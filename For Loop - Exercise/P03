#include <iostream>
#include <climits>
#include <cmath>
using namespace std;

int main()
{
    int n; cin>>n;
    int p1 =0;
    int p2 =0;
    int p3 =0;
    int p4 =0;
    int p5 =0;
    for (int i = 0; i < n; i++) {
       int num; cin>>num;
       if(num<200){
           p1++;
       }else if(num>=200 && num<=399){
           p2++;
       }else if(num>=400&&num<=599){
           p3++;
       }else if(num>=600&&num<=799){
           p4++;
       }else if(num>=800){
           p5++;
       }
    }
    cout.setf(ios::fixed);
    cout.precision(2);
    cout<<1.00*p1/n*100<<"%"<<endl;
    cout<<1.00*p2/n*100<<"%"<<endl;
    cout<<1.00*p3/n*100<<"%"<<endl;
    cout<<1.00*p4/n*100<<"%"<<endl;
    cout<<1.00*p5/n*100<<"%"<<endl;

    return 0;
}
