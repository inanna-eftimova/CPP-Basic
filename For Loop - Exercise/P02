#include <iostream>
#include <climits>
#include <cmath>
using namespace std;

int main()
{
	int n;
	cin >> n;
	int totalSum = 0;
	int maxNumber = INT_MIN;

	for (int i = 0; i < n; i++)
	{
		int number; cin >> number;
		totalSum += number;
		if (number > maxNumber)
		{
			maxNumber = number;
		}
	}
	if (maxNumber == totalSum - maxNumber)
	{
		cout << "Yes" << endl;
		cout << "Sum = " << maxNumber << endl;
	} else {
		cout << "No" << endl;
		cout << "Diff = " << abs(maxNumber - (totalSum - maxNumber));
	}



	return 0;
}
