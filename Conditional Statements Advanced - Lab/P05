#include <iostream>

using namespace std;

int main()
{
    string product; cin>>product;
    string town; cin>>town;
    double kol; cin>>kol;
    double price = 0;
    if(product=="coffee"){
        if(town=="Sofia"){
            price = 0.5;
        }else if(town=="Plovdiv"){
            price=0.4;
        }else{
            price=0.45;
        }
    }else if(product == "water"){
        if(town=="Sofia"){
            price = 0.8;
        }else if(town=="Plovdiv"){
            price=0.7;
        }else{
            price=0.7;
        }
    }else if(product=="beer"){
        if(town=="Sofia"){
            price = 1.2;
        }else if(town=="Plovdiv"){
            price=1.15;
        }else{
            price=1.1;
        }
    }else if(product=="sweets"){
        if(town=="Sofia"){
            price = 1.45;
        }else if(town=="Plovdiv"){
            price=1.30;
        }else{
            price=1.35;
        }
    }else{
        if(town=="Sofia"){
            price = 1.6;
        }else if(town=="Plovdiv"){
            price=1.50;
        }else{
            price=1.55;
        }
    }
    cout<<price*kol<<endl;

    return 0;
}
