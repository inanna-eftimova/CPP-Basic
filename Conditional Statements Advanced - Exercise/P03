#include <iostream>
using namespace std;

int main()
{
    string flowerType; std::cin >> flowerType;
    int flowersNumber; cin >> flowersNumber;
    int budget; cin >> budget;
    double price = 0;


    if (flowerType == "Roses"){
        price = 5;
        if (flowersNumber > 80){
            price *= 0.9;
        }
    }else if (flowerType == "Dahlias" ){
        price = 3.80;
        if (flowersNumber > 90){
            price *= 0.85;
        }
    }else if (flowerType == "Tulips"){
        price = 2.80;
        if (flowersNumber > 80){
            price *= 0.85;
        }
    }else if (flowerType == "Narcissus"){
        price = 3;
        if (flowersNumber < 120){
            price = price * 1.15;
        }
    }else{
        price = 2.50;
        if (flowersNumber < 80){
            price = price * 1.20;
        }
    }

    double totalPrice = flowersNumber * price;

    cout.setf(ios::fixed);
    cout.precision(2);

    if (budget >= totalPrice){
        cout << "Hey, you have a great garden with " << flowersNumber << " " << flowerType <<  " and " << budget - totalPrice << " leva left." << endl;
    }else{
        cout << "Not enough money, you need " << totalPrice - budget << " leva more." << endl;
    }

    return 0;
}
