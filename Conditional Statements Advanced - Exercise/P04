#include <iostream>
using namespace std;

int main()
{
    int groupBudget; cin >> groupBudget;
    string season; std::cin >> season;
    int fishersNumber; cin >> fishersNumber;
    
    double price = 0;

    if (season == "Spring"){
        price = 3000;
    }
    else if (season == "Summer" || season == "Autumn") {
        price = 4200;
    }
    else if (season == "Winter"){
        price = 2600;
    }

    if (fishersNumber <= 6){
        price = price * 0.9;
    }
    else if (fishersNumber >= 7 && fishersNumber <= 11){
        price = price * 0.85;
    }else if (fishersNumber > 12) {
        price = price * 0.75;
    }

    if (fishersNumber % 2 == 0 && season != "Autumn"){
        price = price - (0.05 * price);
    }

    cout.setf(ios::fixed);
    cout.precision(2);


    if (groupBudget >= price) {
        cout << "Yes! You have " << groupBudget - price << " leva left." << endl;
    } 
    else {
      cout << "Not enough money! You need " << price - groupBudget << " leva." << endl;  
    }

    return 0;
}
