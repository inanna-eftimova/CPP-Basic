#include <iostream>

using namespace std;

int main()
{
        string input; getline(cin, input);
        int sumOfSeps = 0;
        while(input!="Going home"){
            int steps = stoi(input);
            sumOfSeps+=steps;
            if(sumOfSeps>=10000){
                cout<<"Goal reached! Good job!"<<endl;
                return 0;
            }
            getline(cin, input);
        }
        int homeSteps; cin>>homeSteps;
        sumOfSeps+=homeSteps;
        if(sumOfSeps>=10000){
                cout<<"Goal reached! Good job!"<<endl;
                return 0;
        }
        cout<<10000-sumOfSeps<<" more steps to reach goal."<<endl;

    return 0;
}
