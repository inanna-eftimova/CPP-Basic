#include <iostream>

using namespace std;

int main()
{
        double neededMoney; cin>>neededMoney;
        double haveMoney; cin>>haveMoney;
        double sum = haveMoney;
        int days = 0;
        int spendingDays = 0;
        while (sum<neededMoney && spendingDays<5){
            string type; cin>>type;
            double pMoney; cin>>pMoney;
            days++;
            if (type=="save"){
                sum+=pMoney;
                spendingDays=0;
            }else if (type=="spend"){
                spendingDays++;
                sum-=pMoney;
                if (sum<0){
                    sum=0;
                }
            }
            if (sum>=neededMoney){
                break;
            }
        }
        if (sum>=neededMoney){
            cout<<"You saved the money for "<<days<<" days."<<endl;
        }else if (spendingDays==5){
            cout<<"You can't save the money."<<endl;
            cout<<days<<endl;
        }

    return 0;
}
